        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": [ "warn", "always", { "omitLastInOneLineBlock": true } ],
